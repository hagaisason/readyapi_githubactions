  <con:parameters>
    <con:parameter required="true">
      <con:name>campaign_id</con:name>
      <con:value/>
      <con:style>TEMPLATE</con:style>
      <con:default/>
      <con:description>Search pattern for a single campaign ID to retrieve vehicles related to this specific campaign ID. The attributes "campaign_id", "import_date" and "criterion" are connected to each other via logical AND operator.</con:description>
